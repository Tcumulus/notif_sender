{
  "expo": {
    "name": "notif_sender",
    "slug": "notif_sender",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./app/assets/alarm.png",
    "splash": {
      "image": "./app/assets/alarm.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/alarm.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "notif_sender.apk"
    },
    "web": {
      "favicon": "./app/assets/alarm.png"
    }
  }
}
